              });
              $el.attr('data-din-applied', 'true');
            }
          });
        });
        
        // Aplicar a todos los enlaces dentro de títulos de productos
        selectors.forEach(function(selector) {
          jQuery(selector + ' a').each(function() {
            var $el = jQuery(this);
            // Solo aplicar si no tiene el estilo D-DIN ya aplicado
            if (!$el.attr('data-din-applied')) {
              $el.css({
                'font-family': '"D-DIN", "DIN", sans-serif',
                'font-size': '14px',
                'color': '#000000',
