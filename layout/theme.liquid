              'background-color': 'transparent'
            });
            $el.attr('data-bg-transparent', 'true');
          }
        });
      }
    }
    
    // Asegurar que los enlaces de imágenes funcionen correctamente
    function ensureImageLinks() {
      if (typeof jQuery !== 'undefined') {
        // Asegurar que todos los enlaces .engoj_find_img funcionen
        jQuery('.engoj_find_img').each(function() {
          var $link = jQuery(this);
          var href = $link.attr('href');
          
          // Si no tiene href, intentar obtenerlo del producto
          if (!href || href === '#' || href === 'javascript:void(0)') {
            var $product = $link.closest('.product, .engoj_grid_parent, .product-item-v1, .product-item-v2, .product-item-v3, .product-item-v4, .product-item-v5, .product-item-v6, .product-item-v7, .product-item-v8, .product-item-v9');
            var $titleLink = $product.find('.title-product a');
            if ($titleLink.length > 0) {
              href = $titleLink.attr('href');
              if (href) {
                $link.attr('href', href);
              }
            }
          }
          
          // Asegurar estilos CSS para que sea clickeable
          $link.css({
            'display': 'block',
            'position': 'relative',
            'z-index': '2',
            'cursor': 'pointer'
          });
          
          // Asegurar que los elementos superpuestos no bloqueen
          $link.closest('.img-product').find('.label-sale, .label-new, .countdown-timer, .sold-out').css({
            'pointer-events': 'none'
          });
          
          // Asegurar que el click funcione - redirección manual como fallback
          $link.off('click.force-link').on('click.force-link', function(e) {
            // Obtener href actualizado
            href = $link.attr('href');
            
            // Si no hay href válido, intentar obtenerlo del título
            if (!href || href === '#' || href === 'javascript:void(0)') {
              var $product = $link.closest('.product, .engoj_grid_parent');
              var $titleLink = $product.find('.title-product a');
              if ($titleLink.length > 0) {
                href = $titleLink.attr('href');
              }
            }
            
            // Si hay href válido, redirigir manualmente para asegurar que funcione
            if (href && href !== '#' && href !== 'javascript:void(0)') {
              e.preventDefault();
              e.stopPropagation();
              window.location.href = href;
              return false;
            } else {
              e.preventDefault();
              return false;
            }
          });
          
          // También agregar click directo en la imagen como fallback
          $link.find('img').off('click.force-link-img').on('click.force-link-img', function(e) {
            e.stopPropagation();
            href = $link.attr('href');
            if (!href || href === '#' || href === 'javascript:void(0)') {
              var $product = $link.closest('.product, .engoj_grid_parent');
              var $titleLink = $product.find('.title-product a');
              if ($titleLink.length > 0) {
                href = $titleLink.attr('href');
              }
            }
            if (href && href !== '#' && href !== 'javascript:void(0)') {
              e.preventDefault();
              window.location.href = href;
            }
          });
        });
      }
    }
    
    // Ejecutar inmediatamente si jQuery ya está disponible
    if (typeof jQuery !== 'undefined') {
      forceGlobalTypography();
      ensureImageLinks();
      jQuery(document).ready(function() {
        forceGlobalTypography();
        ensureImageLinks();
        // Ejecutar continuamente cada 200ms para asegurar que se aplique
        setInterval(function() {
          forceGlobalTypography();
          ensureImageLinks();
        }, 200);
      });
    } else {
      window.addEventListener('load', function() {
        if (typeof jQuery !== 'undefined') {
          forceGlobalTypography();
          ensureImageLinks();
          setInterval(function() {
            forceGlobalTypography();
            ensureImageLinks();
          }, 200);
        }
      });
    }
    
    // Ejecutar también después de cargas dinámicas
    if (typeof jQuery !== 'undefined') {
      jQuery(document).on('DOMNodeInserted DOMSubtreeModified', function() {
        forceGlobalTypography();
        ensureImageLinks();
      });
      
      // Observer para cambios en el DOM
      if (typeof MutationObserver !== 'undefined') {
        var observer = new MutationObserver(function(mutations) {
          forceGlobalTypography();
          ensureImageLinks();
        });
        observer.observe(document.body, {
          childList: true,
          subtree: true
        });
      }
    }
  })();
  </script>

