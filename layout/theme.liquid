        jQuery('.product-item-v1, .product-item-v2, .product-item-v3, .product-item-v4, .product-item-v5, .product-item-v6, .product-item-v7, .product-item-v8, .product-item-v9, .info-product, .col-md-4, .col-md-6, .col-lg-12, .product_item').each(function() {
